{
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": ["backend/**", "frontend/**", "requirements.txt"]
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/backend/main.py"
    }
  ]
}

